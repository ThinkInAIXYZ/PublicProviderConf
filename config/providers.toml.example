# PublicProviderConf Configuration File Example
# 
# SECURITY WARNING: This is an example file!
# 1. Copy this file to 'providers.toml' in the same directory
# 2. Edit 'providers.toml' with your actual settings and API keys
# 3. The actual 'providers.toml' file is ignored by git to prevent API key leaks
#
# Usage: cp config/providers.toml.example config/providers.toml
#
# This file allows you to customize provider settings and API keys

[providers.ppinfra]
api_url = "https://api.ppinfra.com/openai/v1/models"
# PPInfra doesn't require an API key
rate_limit = 10
timeout = 30

[providers.openrouter]
api_url = "https://openrouter.ai/api/v1/models"
# OpenRouter provides public model listing API - no API key required
rate_limit = 5
timeout = 30

[providers.gemini]
api_url = "https://generativelanguage.googleapis.com/v1beta/openai/models"
# Option 1: Use environment variable (recommended for security)
api_key_env = "GEMINI_API_KEY"
# Option 2: Direct API key (not recommended for production)
# api_key = "your-gemini-key-here"
# Option 3: Use custom environment variable name
# api_key_env = "MY_CUSTOM_GEMINI_KEY"
rate_limit = 10
timeout = 60  # Web scraping might take longer

[providers.vercel]
api_url = "https://ai-gateway.vercel.sh/v1/models"
# Vercel AI Gateway provides public model listing API - no API key required
rate_limit = 10
timeout = 30

[providers.github_ai]
api_url = "https://models.inference.ai.azure.com/models"
# GitHub AI Models provides public model listing API - no API key required
rate_limit = 5
timeout = 30

[providers.tokenflux]
api_url = "https://tokenflux.ai/v1/models"
# Tokenflux provides public model listing API - no API key required
rate_limit = 10
timeout = 30

[providers.groq]
api_url = "https://api.groq.com/openai/v1/models"
# Option 1: Use environment variable (recommended for security)
api_key_env = "GROQ_API_KEY"
# Option 2: Direct API key (not recommended for production)
# api_key = "your-groq-key-here"
# Option 3: Use custom environment variable name
# api_key_env = "MY_CUSTOM_GROQ_KEY"
rate_limit = 10
timeout = 30

[providers.deepseek]
api_url = "https://api-docs.deepseek.com/quick_start/pricing"
# DeepSeek uses web scraping from documentation - no API key required
rate_limit = 5
timeout = 60  # Web scraping might take longer

# API Key Configuration Notes:
# - PPInfra: Uses public API, no key required
# - OpenRouter: Uses public model listing API, no key required  
# - Vercel: Uses public AI Gateway API, no key required
# - GitHub AI: Uses public model listing API, no key required
# - Tokenflux: Uses public API, no key required
# - Gemini: Optional API key for complete model list
# - Groq: Requires API key for access
# - DeepSeek: Uses web scraping from documentation, no key required
#
# API Key Priority (when applicable):
# 1. Direct API key in config file (api_key)
# 2. Environment variable specified in config (api_key_env)  
# 3. Default environment variable (e.g., GEMINI_API_KEY, GROQ_API_KEY)
